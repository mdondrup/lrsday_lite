augustus=$bin_dir/augustus #location of augustus executable
fgenesh= #location of fgenesh executable
evm=$bin_dir/evidence_modeler.pl #location of EvidenceModeler executable
tRNAscan-SE=$bin_dir/tRNAscan-SE #location of trnascan executable
snoscan=$bin_dir/snoscan #location of snoscan executable

#-----Other Algorithms
probuild= #location of probuild executable (required for genemark)
